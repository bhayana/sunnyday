<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
 
 <hibernate-mapping>
     <class name="com.javapapers.spring.mvc.NgoInvoiceDetail" 
         table="ngo_invoices_details">
       
       <id name="id">  
     <generator class="assigned"></generator>  
    </id>  
    <property name="randomno"></property>
         <property name="username"></property>
         <property name="invoicedescription"></property>
         <property name="invoicetitle"></property>
         <property name="invoice"></property>
         <property name="invoiceamount"></property>
   
     </class>
     
 </hibernate-mapping>
 